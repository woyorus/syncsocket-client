{
  "extends": "standard",
  "parser": "babel-eslint",
  "env": {
    "node": true
  },
  "rules": {
    "yoda": 0,
    "semi": [2, "always"],
    "no-extra-semi": 2,
    "semi-spacing": [2, { "before": false, "after": true }],
    "indent": ["error", 4, { "SwitchCase": 1 }],
    "space-before-function-paren": ["error", { "anonymous": "ignore", "named": "never" }]
  }
}
